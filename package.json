{
  "name": "homebridge-isy",
  "displayName": "Homebridge ISY",
  "version": "0.5.0-beta.27",
  "description": "Homebridge plugin for ISY series of home controllers from Universal Devices, Inc.",
  "note": "",
  "main": "dist/plugin.js",
  "scripts": {
    "test": "mocha",
    "start": "DEBUG=* homebridge -D -P ./",
    "build": "bash ./build.sh",
    "cleanup": "rm -r tmp",
    "prepublishOnly": "npm run build",
    "debug": "npm prepare npm start",
    "moduleRedirect": "./node_modules/.bin/babel tmp -d dist",
    "postinstall": "npm run moduleRedirect; npm run cleanup"
  },
  "engines": {
    "node": ">12.0.0",
    "homebridge": ">=0.4.53"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeepmouli/homebridge-isy-js.git"
  },
  "files": [
    "tmp",
    "typings",
    "config.schema.json",
    "sampleconfig.json",
    "install.sh",
    "LICENSE",
    "README.md",
    "babel.config.js"
  ],
  "keywords": [
    "homebridge-plugin",
    "isy-js",
    "isy",
    "Home Center",
    "homekit",
    "insteon",
    "Siri"
  ],
  "devDependencies": {
    "@types/node": "",
    "@types/restler": "",
    "fake-isy-994i": "",
    "hap-nodejs": "^0.6.4",
    "homebridge": "^1.0.1",
    "mocha": "",
    "npm": "^6.14.4",
    "p-try": "^2.2.0",
    "path-exists": "^4.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@log4js-node/log4js-api": "^1.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "faye-websocket": "^0.11.3",
    "isy-nodejs": "^0.5.0-beta.38",
    "path": "^0.12.7",
    "restler": "^3.4.0",
    "xmldoc": "^1.1.2"
  },
